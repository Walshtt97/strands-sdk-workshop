# AWS Configuration for A2A Service
# Copy this file to .env and fill in your AWS credentials

# AWS Credentials (required)
# AWS_ACCESS_KEY_ID=your_access_key_here
# AWS_SECRET_ACCESS_KEY=your_secret_key_here
# AWS_SESSION_TOKEN=your_session_token_here (only if using temporary credentials)

# AWS Region (required)
AWS_REGION=us-east-1

# Bedrock Model Configuration (optional)
BEDROCK_MODEL_ID=us.anthropic.claude-sonnet-4-20250514-v1:0

# User identifier for knowledge base naming (optional)
USER_NAME=workshop